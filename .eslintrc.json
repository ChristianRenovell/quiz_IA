{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:react/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "indent": "off",
        "prettier/prettier": "error",
        "eslint-disable no-debugger": "off",
        "eslint-disable no-unused-vars": "off",
        "no-debugger": "off"
    }
}
